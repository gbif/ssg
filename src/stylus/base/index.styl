@import "_vars"

a
	text-decoration: none
	color: ink-color

.overlay
	position:absolute
	z-index: 50
	top:0px
	left:0px
	right:0px
	bottom:0px
	height:100%
	display:none

@media (max-width: laptop)
	body.hasOverlay
		position: fixed;
		overflow-y: hidden;
		width: 100%;
		>.overlay
			display: block