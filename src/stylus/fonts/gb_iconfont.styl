@font-face {
	font-family: 'gbificons';
	src:url('/css/fonts/iconfont/gbificons.eot');
	src:url('/css/fonts/iconfont/gbificons.eot#iefix') format('embedded-opentype'),
	url('/css/fonts/iconfont/gbificons.ttf') format('truetype'),
	url('/css/fonts/iconfont/gbificons.woff') format('woff');
	font-weight: normal;
	font-style: normal;
}

[class^="gb-icon-"]:before, [class*=" gb-icon-"]:before {
	font-family: 'gbificons';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}



